{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "nuvtools-common/src/NuvTools.Common/bin/**/*.dll",
            "nuvtools-data/src/NuvTools.Data/bin/**/*.dll",
            "nuvtools-data/src/NuvTools.Data.EntityFrameworkCore/bin/**/*.dll",
            "nuvtools-data/src/NuvTools.Data.EntityFrameworkCore.SqlServer/bin/**/*.dll",
            "nuvtools-aspnetcore/src/NuvTools.AspNetCore/bin/**/*.dll",
            "nuvtools-aspnetcore/src/NuvTools.AspNetCore.EntityFrameworkCore/bin/**/*.dll",
            "nuvtools-notification/src/NuvTools.Notification.Mail/bin/**/*.dll",
            "nuvtools-notification/src/NuvTools.Notification.Mail.Smtp/bin/**/*.dll",
            "nuvtools-report/src/NuvTools.Report/bin/**/*.dll",
            "nuvtools-report/src/NuvTools.Report.Pdf/bin/**/*.dll",
            "nuvtools-report/src/NuvTools.Report.Sheet/bin/**/*.dll",
            "nuvtools-security/src/NuvTools.Security/bin/**/*.dll",
            "nuvtools-security/src/NuvTools.Security.AspNetCore/bin/**/*.dll",
            "nuvtools-security/src/NuvTools.Security.Models/bin/**/*.dll",
            "nuvtools-security-identity/src/NuvTools.Security.Identity/bin/**/*.dll",
            "nuvtools-security-identity/src/NuvTools.Security.Identity.AspNetCore/bin/**/*.dll",
            "nuvtools-security-identity/src/NuvTools.Security.Identity.Models/bin/**/*.dll",
            "nuvtools-storage/src/NuvTools.Storage/bin/**/*.dll",
            "nuvtools-storage/src/NuvTools.Storage.Azure/bin/**/*.dll",
            "nuvtools-validation/src/NuvTools.Validation/bin/**/*.dll",
            "nuvtools-resources/src/NuvTools.Resources/bin/**/*.dll"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "outputFormat": "mref",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "enumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md",
          "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "Nuv Tools Framework Docs",
      "_appName": "Framework Docs",
      "_enableSearch": "true",
      "_appLogoPath": "images/NuvToolsLogoLandscape.svg",
      "_appFaviconPath": "images/icon.png",
      "_appFooter": "<span>Supported by the <a href=\"https://nuv.tools\" target=\"_blank\">Nuv Tools</a>. Documentation generated by <a href=\"https://dotnet.github.io/docfx\" target=\"_blank\">docfx</a></span>."
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],

    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}